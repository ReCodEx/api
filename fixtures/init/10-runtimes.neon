App\Model\Entity\RuntimeEnvironment:
	c-gcc-linux:
		__construct:
			- c-gcc-linux
			- C
			- "C (GCC)"
			- "[c, h]"
			- GNU/Linux
			- C language compiled with GCC
			- "- name: \"source-files\"\n  type: \"file[]\"\n  value: \"*.{c,h}\""

	cxx-gcc-linux:
		__construct:
			- cxx-gcc-linux
			- C++
			- "C++ (GCC)"
			- "[cpp, h, hpp]"
			- GNU/Linux
			- C++17 language compiled with GCC 7.3
			- "- name: \"source-files\"\n  type: \"file[]\"\n  value: \"*.{cpp,h,hpp}\""

	freepascal-linux:
		__construct:
			- freepascal-linux
			- Pascal
			- "Pascal (FreePascal)"
