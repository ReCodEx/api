App\Model\Entity\PipelineConfig:
    demoCompilationPipelineConfig:
        __construct:
            - '''
              variables:
                - name: source_files
                  type: file[]
                  value:
                    - source.cpp
                - name: binary_file
                  type: file
                  value: a.out
              boxes:
                - name: source
                  type: file-in
                  portsIn: []
                  portsOut:
                    input:
                       type: file
                       value: source_files
                - name: compilation
                  type: gcc
                  portsIn:
                    source-files:
                      type: file[]
                      value: source_files
                    args:
                      type: string[]
                      value: ""
                  portsOut:
                    binary-file:
                      type: file
                      value: binary_file
                - name: output
                  type: file-out
                  portsOut: []
                  portsIn:
                    output:
                       type: file
                       value: binary_file
              '''
            - @demoAdmin
    demoTestPipelineConfig:
        __construct:
            - '''
              variables:
                - name: binary_file
                  type: file
                  value: a.out
                - name: input_files
                  type: file[]
                  value:
                    - input.in
                - name: expected_output
                  type: file
                  value: expected.out
                - name: actual_output
                  type: file
                  value: actual.out
              boxes:
                - name: binary
                  portsIn: []
                  portsOut:
                    input:
                       type: file
                       value: binary_file
                  type: file-in
                - name: input
                  portsIn: []
                  portsOut:
                    input:
                       type: file
                       value: input_file
                  type: file-in
                - name: test
                  portsIn: []
                  portsOut:
                    input:
                      type: file
                      value: expected_output
                  type: file-in
                - name: run
                  type: elf-exec
                  portsIn:
                    args:
                      type: string[]
                      value: ""
                    stdin:
                      type: file
                      value: ""
                    binary-file:
                      type: file
                      value: binary_file
                    input-files:
                      type: file[]
                      value: input_files
                  portsOut:
                    stdout:
                      type: file
                      value: ""
                    output-file:
                      type: file
                      value: actual_output
                - name: judge
                  portsIn:
                    actual-output:
                      type: file
                      value: actual_output
                    expected-output:
                      type: file
                      value: expected_output
                  portsOut: []
                  type: judge-normal
              '''
            - @demoAdmin

App\Model\Entity\Pipeline:
    demoPipelineA:
        __construct:
            create:
                - @demoAdmin
        id: compilationPipeline
        name: "compilationPipeline"
        pipelineConfig: @demoCompilationPipelineConfig

    demoPipelineB:
        __construct:
            create:
                - @demoAdmin
        id: testPipeline
        name: "testPipeline"
        pipelineConfig: @demoTestPipelineConfig
