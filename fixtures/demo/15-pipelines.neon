App\Model\Entity\PipelineConfig:
    demoPipelineConfig:
        __construct:
            - '''
              variables:
                - name: varA
                  type: string
                  value: valA
              boxes:
                - name: source
                  portsIn: []
                  portsOut:
                    in-data:
                       type: file
                       value: source_file
                  type: data-in
                - name: test
                  portsIn: []
                  portsOut:
                    in-data:
                      type: file
                      value: test_file
                  type: data-in
                - name: compilation
                  portsIn:
                    source-file:
                      type: file
                      value: source_file
                  portsOut:
                    binary-file:
                      type: file
                      value: binary_file
                  type: gcc
                - name: run
                  portsIn:
                    binary-file:
                      type: file
                      value: binary_file
                  portsOut:
                    output-file:
                      type: file
                      value: actual_output
                  type: elf-exec
                - name: judge
                  portsIn:
                    actual-output:
                      type: file
                      value: actual_output
                    expected-output:
                      type: file
                      value: expected_output
                  portsOut:
                    score:
                      type: string
                      value: score
                  type: judge-normal
              '''
            - @demoAdmin

App\Model\Entity\Pipeline:
    demoPipelineA:
        __construct:
            create:
                - @demoAdmin
        id: pipelineA
        name: "compilationPipeline"
        pipelineConfig: @demoPipelineConfig

    demoPipelineB:
        __construct:
            create:
                - @demoAdmin
        id: pipelineB
        name: "testPipeline"
        pipelineConfig: @demoPipelineConfig
