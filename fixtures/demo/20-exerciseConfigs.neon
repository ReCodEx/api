App\Model\Entity\ExerciseEnvironmentConfig:
    demoEnvironmentConfig:
        __construct:
            - @CRuntime
            - '''
              - name: "source_file"
                type: "file"
                value: "source.cpp"
              '''
            - @demoAdmin
    demoJavaEnvironmentConfig:
        __construct:
            - @Java8Runtime
            - '''
              - name: "source_file"
                type: "file"
                value: "source.java"
              '''
            - @demoAdmin


App\Model\Entity\ExerciseConfig:
    demoExerciseConfig:
        __construct:
            - '''
              environments:
                - java8
                - c-gcc-linux
              tests:
                "Test 1":
                  pipelines:
                    - name: "compilationPipeline"
                      variables:
                        - name: expected_output
                          type: file
                          value: expected.1.out
                  environments:
                    java8:
                      pipelines:
                        - name: "compilationPipeline"
                          variables:
                            - name: expected_output
                              type: file
                              value: expected.1.java.out
                    c-gcc-linux: []
                "Test 2":
                  pipelines:
                    - name: "compilationPipeline"
                      variables:
                        - name: expected_output
                          type: file
                          value: expected.2.out
                  environments:
                    c-gcc-linux:
                      pipelines:
                        - name: "compilationPipeline"
                          variables:
                            - name: expected_output
                              type: file
                              value: expected.2.cpp.out
              '''
            - @demoAdmin
