App\Model\Entity\ExerciseEnvironmentConfig:
    demoEnvironmentConfig:
        __construct:
            - @CRuntime
            - '''
              - name: "source-files"
                type: "file[]"
                value: "*.c"
              '''
            - @demoAdmin
    demoJavaEnvironmentConfig:
        __construct:
            - @JavaRuntime
            - '''
              - name: "source-files"
                type: "file[]"
                value: "*.java"
              '''
            - @demoAdmin

App\Model\Entity\ExerciseTest:
    demoExerciseTest1:
        __construct:
            - "Test 1"
            - "Description of Test 1"
            - @demoAdmin
    demoExerciseTest2:
        __construct:
            - "Test 2"
            - "Description of Test 2"
            - @demoAdmin

App\Model\Entity\ExerciseConfig:
    demoExerciseConfig:
        __construct:
            - '''
              environments:
                - java
                - c-gcc-linux
              tests:
                "Test 1":
                  pipelines:
                    - name: "compilationPipeline"
                      variables:
                        - name: expected-output
                          type: remote-file
                          value: expected.1.out
                  environments:
                    java:
                      pipelines:
                        - name: "compilationPipeline"
                          variables:
                            - name: expected-output
                              type: remote-file
                              value: expected.1.java.out
                    c-gcc-linux: []
                "Test 2":
                  pipelines:
                    - name: "compilationPipeline"
                      variables:
                        - name: expected-output
                          type: remote-file
                          value: expected.2.out
                  environments:
                    c-gcc-linux:
                      pipelines:
                        - name: "compilationPipeline"
                          variables:
                            - name: expected-output
                              type: remote-file
                              value: expected.2.cpp.out
                    java: []
              '''
            - @demoAdmin
