App\Model\Entity\ExerciseEnvironmentConfig:
    demoEnvironmentConfig:
        __construct:
            - @CRuntime
            - '''
              "varA":
                type: "file"
                value: "valA"
              "varB":
                type: "string"
                value: "valB"
              '''
    demoJavaEnvironmentConfig:
        __construct:
            - @Java8Runtime
            - '''
              "varA":
                type: "file"
                value: "valA"
              "varB":
                type: "string"
                value: "valB"
              '''


App\Model\Entity\ExerciseConfig:
    demoExerciseConfig:
        __construct:
            - '''
              environments:
                - java8
                - c-gcc-linux
              tests:
                "Test 1":
                  pipelines:
                    "pipeline1":
                      variables:
                        varA:
                          type: string
                          value: valA
                  environments:
                    java8:
                      pipelines:
                        "pipelineJava":
                          variables:
                            varJava:
                              type: string
                              value: valJava
                    c-gcc-linux: []
                "Test 2":
                  pipelines:
                    "pipeline2":
                      variables:
                        varB:
                          type: file
                          value: valB
                  environments:
                    c-gcc-linux:
                      pipelines:
                        "pipeline2":
                          variables:
                            varCpp:
                              type: file
                              value: valCpp
              '''
