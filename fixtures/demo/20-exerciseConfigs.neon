App\Model\Entity\ExerciseEnvironmentConfig:
    demoEnvironmentConfig:
        __construct:
            - @CRuntime
            - '''
              - name: "varA"
                type: "file"
                value: "valA"
              - name: "varB"
                type: "string"
                value: "valB"
              '''
            - @demoAdmin
    demoJavaEnvironmentConfig:
        __construct:
            - @Java8Runtime
            - '''
              - name: "varA"
                type: "file"
                value: "valA"
              - name: "varB"
                type: "string"
                value: "valB"
              '''
            - @demoAdmin


App\Model\Entity\ExerciseConfig:
    demoExerciseConfig:
        __construct:
            - '''
              environments:
                - java8
                - c-gcc-linux
              tests:
                "Test 1":
                  pipelines:
                    "compilationPipeline":
                      variables:
                        - name: varA
                          type: string
                          value: valA
                  environments:
                    java8:
                      pipelines:
                        "compilationPipeline":
                          variables:
                            - name: varJava
                              type: string
                              value: valJava
                    c-gcc-linux: []
                "Test 2":
                  pipelines:
                    "compilationPipeline":
                      variables:
                        - name: varB
                          type: file
                          value: valB
                  environments:
                    c-gcc-linux:
                      pipelines:
                        "compilationPipeline":
                          variables:
                            - name: varCpp
                              type: file
                              value: valCpp
              '''
            - @demoAdmin
