App\Model\Entity\LocalizedGroup:
    demoGroupText:
        __construct:
            - "en"
            - Demo group
            - A group meant solely for the purpose of demonstration
    demoChildGroupText:
        __construct:
            - "en"
            - Demo child group
            - A group meant solely for the purpose of demonstration of child groups
    demoPrivateGroupText:
        __construct:
            - "en"
            - Private group
            - A group meant solely for the purpose of demonstration of private groups
    demoArchivedGroupText:
        __construct:
            - "en"
            - "Archived group"
            - "Old group which already has been archived"
    demoArchivedXOGroupText:
        __construct:
            - "en"
            - "Archived group (Extra Old)"
            - "Very old group which already has been archived long time ago (in a galaxy far far away)"

App\Model\Entity\Group:
    demoGroup:
        __construct:
            - NPRG031
            - @demoInstance
            - @demoAdmin
        addLocalizedText: @demoGroupText
    demoChildGroup:
        __construct:
            - NPRG031
            - @demoInstance
            - @demoAdmin
            - @demoGroup
        addLocalizedText: @demoChildGroupText
    demoPrivateGroup:
        __construct:
            - NPRG031
            - @demoInstance
            - @demoAdmin
            - NULL
            - TRUE
            - FALSE
        addLocalizedText: @demoPrivateGroupText
    demoArchivedGroup:
        __construct:
            - OLD002
            - @demoInstance
            - @demoAdmin
            - @demoGroup
        addLocalizedText: @demoArchivedGroupText
        archive: "<dateTimeBetween('-10 months', '-1 months')>"
    demoArchivedXOGroup:
        __construct:
            - OLD001
            - @demoInstance
            - @demoAdmin
            - @demoGroup
        addLocalizedText: @demoArchivedXOGroupText
        archive: "<dateTimeBetween('-5 years', '-3 years')>"

App\Model\Entity\User:
    'demoGroupMember{1..10}':
        __construct:
            - 'demoGroupMember<current()>\@example.com'
            - "<firstName()>"
            - "<lastName()>"
            - ""
            - ""
            - student
            - @demoInstance
        makeStudentOf: @demoGroup
        setVerified: true

    demoGroupSupervisor:
        __construct:
            - "demoGroupSupervisor@example.com"
            - "<firstName()>"
            - "<lastName()>"
            - ""
            - ""
            - supervisor
            - @demoInstance
        makeSupervisorOf: @demoGroup
        setVerified: true

    demoGroupSupervisor2:
        __construct:
            - "demoGroupSupervisor2@example.com"
            - "<firstName()>"
            - "<lastName()>"
            - ""
            - ""
            - supervisor
            - @demoInstance
        makeSupervisorOf: @demoChildGroup
        setVerified: true

    anotherSupervisor:
        __construct:
            - "anotherSupervisor@example.com"
            - "<firstName()>"
            - "<lastName()>"
            - ""
            - ""
            - supervisor
            - @demoInstance
        setVerified: true

    "submitUser1":
        __construct: ["submitUser1@example.com", "<firstName()>", "<lastName()>", "", "", student, @demoInstance]
        makeStudentOf: @demoGroup
        setVerified: true
    
    nonmemberStudent:
        __construct: ["nonmemberStudent@example.com", "<firstName()>", "<lastName()>", "", "", student, @demoInstance]
        setVerified: true


App\Model\Entity\Login:
    submitUser1Login:
        __construct:
            createLogin:
                - "@submitUser1"
                - "submitUser1@example.com"
                - "password"
    demoGroupSupervisorLogin:
        __construct:
            createLogin:
                - @demoGroupSupervisor
                - "demoGroupSupervisor@example.com"
                - "password"
    demoGroupSupervisor2Login:
        __construct:
            createLogin:
                - @demoGroupSupervisor2
                - "demoGroupSupervisor2@example.com"
                - "password"
    anotherSupervisorLogin:
        __construct:
            createLogin:
                - @anotherSupervisor
                - "anotherSupervisor@example.com"
                - "password"
    'demoGroupMemberLogin{1..10}':
        __construct:
            createLogin:
                - "@demoGroupMember<current()>"
                - 'demoGroupMember<current()>\@example.com'
                - "password"

App\Model\Entity\ExternalLogin:
    demoGroupMemberExternalLogin:
        __construct:
            - @demoGroupMember1
            - 'ext-service'
            - 'external-login1'

App\Model\Entity\GroupInvitation:
    validInviation:
        __construct:
            - @demoChildGroup
            - @demoAdmin
            - null
            - "a1"
    invalidInviation:
        __construct:
            - @demoChildGroup
            - @demoAdmin
            - "<dateTimeBetween('-2 months', '-1 months')>"
            - "f3"

