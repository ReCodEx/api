App\Model\Entity\LocalizedGroup:
    demoGroupText:
        __construct:
            - "en"
            - Demo group
            - A group meant solely for the purpose of demonstration
    demoChildGroupText:
        __construct:
            - "en"
            - Demo child group
            - A group meant solely for the purpose of demonstration of child groups
    demoPrivateGroupText:
        __construct:
            - "en"
            - Private group
            - A group meant solely for the purpose of demonstration of private groups
App\Model\Entity\Group:
    demoGroup:
        __construct:
            - NPRG031
            - @demoInstance
            - @demoAdmin
        addLocalizedText:
            - @demoGroupText
    demoChildGroup:
        __construct:
            - NPRG031
            - @demoInstance
            - @demoAdmin
            - @demoGroup
        addLocalizedText:
            - @demoChildGroupText
    demoPrivateGroup:
        __construct:
            - NPRG031
            - @demoInstance
            - @demoAdmin
            - NULL
            - TRUE
            - FALSE
        addLocalizedText:
            - @demoPrivateGroupText

App\Model\Entity\User:
    'demoGroupMember{1..10}':
        __construct:
            - "demoGroupMember<current()>@example.com"
            - "<firstName()>"
            - "<lastName()>"
            - ""
            - ""
            - student
            - @demoInstance
        makeStudentOf:
            - @demoGroup

    demoGroupSupervisor:
        __construct:
            - "demoGroupSupervisor@example.com"
            - "<firstName()>"
            - "<lastName()>"
            - ""
            - ""
            - supervisor
            - @demoInstance
        makeSupervisorOf:
            - @demoGroup

    demoGroupSupervisor2:
        __construct:
            - "demoGroupSupervisor2@example.com"
            - "<firstName()>"
            - "<lastName()>"
            - ""
            - ""
            - supervisor
            - @demoInstance
        makeSupervisorOf:
            - @demoGroup

    "submitUser1":
        __construct: ["submitUser1@example.com", "<firstName()>", "<lastName()>", "", "", student, @demoInstance]
        makeStudentOf: [@demoGroup]

App\Model\Entity\Login:
    submitUser1Login:
        __construct:
            createLogin:
                - "@submitUser1"
                - "submitUser1@example.com"
                - "password"
    demoGroupSupervisorLogin:
        __construct:
            createLogin:
                - @demoGroupSupervisor
                - "demoGroupSupervisor@example.com"
                - "password"
    demoGroupSupervisor2Login:
        __construct:
            createLogin:
                - @demoGroupSupervisor2
                - "demoGroupSupervisor2@example.com"
                - "password"
