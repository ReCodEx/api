App\Model\Entity\SolutionEvaluation:
    "evaluation{1..4}":
        __construct: false
        evaluatedAt: "<dateTime()>"
        initFailed: false
        score: 0.95
        scoreConfig: @weightedTwoTests
        points: 10
        initiationOutputs: ""
        testResults: []

App\Model\Entity\Solution:
     "demoSolution{1..2}":
        __construct:
            - @submitUser1
            - @CRuntime

App\Model\Entity\SolutionFile:
    "uploadedFile{1..2}":
        __construct:
            - "filename.ext"
            - "<dateTime()>"
            - 0
            - @submitUser1
            - "some/file/path/filename.ext"
            - "@demoSolution<current()>"

App\Model\Entity\AssignmentSolution:
    "solution1":
        __construct:
            createSolution:
                - "Random note"
                - @demoAssignment
                - "@demoSolution1"
        bonusPoints: 1
        lastSubmission: @submission6
    "solution2":
        __construct:
            createSolution:
                - "Random note"
                - @demoAssignment
                - "@demoSolution2"
        bonusPoints: 1
        lastSubmission: @submission4

App\Model\Entity\SubmissionFailure:
    "resolvedSubmissionFailure":
        __construct:
            create:
                - evaluation_failure
                - Things went south
                - "<dateTime()>"
        resolve:
            - Got it!
    unresolvedSubmissionFailure:
        __construct:
            create:
                - evaluation_failure
                - Things went south
                - "<dateTime()>"

App\Model\Entity\AssignmentSolutionSubmission:
    "submission{1..2}":
        __construct:
            - "@solution1"
            - "jobConfigPath"
            - @submitUser1
        resultsUrl: localhost/results/hehe.zip
        evaluation: "@evaluation<current()>"
    "submission{3..4}":
        __construct:
            - "@solution2"
            - "jobConfigPath"
            - @submitUser1
        resultsUrl: localhost/results/hehe.zip
        evaluation: "@evaluation<current()>"
    "submission5":
        __construct:
            - "@solution1"
            - "jobConfigPath"
            - @submitUser1
        resultsUrl: localhost/results/hehe.zip
        failure: "@resolvedSubmissionFailure"
    "submission6":
        __construct:
            - "@solution1"
            - "jobConfigPath"
            - @submitUser1
        resultsUrl: localhost/results/hehe.zip
        failure: "@unresolvedSubmissionFailure"
