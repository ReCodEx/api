App\Model\Entity\SolutionEvaluation:
    "evaluation{1..2}":
        __construct: false
        bonusPoints: 1
        evaluatedAt: "<dateTime()>"
        initFailed: false
        score: 0.95
        points: 10
        isValid: true
        evaluationFailed: false
        resultYml: ""
        testResults: []

App\Model\Entity\Solution:
     "demoSolution{1..2}":
        __construct:
            - @submitUser1
            - @demoRuntimeConfig

App\Model\Entity\SolutionFile:
    "uploadedFile{1..2}":
        __construct:
            - "filename.ext"
            - "<dateTime()>"
            - 0
            - @submitUser1
            - "some/file/path/filename.ext"
            - "@demoSolution<current()>"

App\Model\Entity\Submission:
    "submission{1..2}":
        __construct:
            createSubmission:
                - "Random note"
                - @demoAssignment
                - @submitUser1
                - @submitUser1
                - "@demoSolution<current()>"
                - false
        resultsUrl: localhost/results/hehe.zip
        evaluation: "@evaluation<current()>"

App\Model\Entity\SubmissionFailure:
    "resolvedSubmissionFailure{1..2}":
        __construct:
            - evaluation_failure
            - Things went south
            - "@submission<current()>"
            - "<dateTime()>"
        resolve:
            - Got it!
    unresolvedSubmissionFailure:
        __construct:
            - evaluation_failure
            - Things went south
            - @submission1
            - "<dateTime()>"
