App\Model\Entity\SolutionEvaluation:
    "evaluation{1..2}":
        __construct: false
        evaluatedAt: "<dateTime()>"
        initFailed: false
        score: 0.95
        points: 10
        initiationOutputs: ""
        testResults: []

App\Model\Entity\Solution:
     "demoSolution{1..2}":
        __construct:
            - @submitUser1
            - @CRuntime

App\Model\Entity\SolutionFile:
    "uploadedFile{1..2}":
        __construct:
            - "filename.ext"
            - "<dateTime()>"
            - 0
            - @submitUser1
            - "some/file/path/filename.ext"
            - "@demoSolution<current()>"

App\Model\Entity\AssignmentSolution:
    "solution{1..2}":
        __construct:
            createSolution:
                - "Random note"
                - @demoAssignment
                - "@demoSolution<current()>"
        bonusPoints: 1

App\Model\Entity\AssignmentSolutionSubmission:
    "submission{1..2}":
        __construct:
            - "@solution<current()>"
            - "jobConfigPath"
            - @submitUser1
        resultsUrl: localhost/results/hehe.zip
        evaluation: "@evaluation<current()>"

App\Model\Entity\SubmissionFailure:
    "resolvedSubmissionFailure":
        __construct:
            forSubmission:
                - evaluation_failure
                - Things went south
                - @submission1
                - "<dateTime()>"
        resolve:
            - Got it!
    unresolvedSubmissionFailure:
        __construct:
            forSubmission:
                - evaluation_failure
                - Things went south
                - @submission1
                - "<dateTime()>"
