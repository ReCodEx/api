App\Model\Entity\ExerciseScoreConfig:
    weightedTwoTests:
        __construct:
            - 'weighted'
            - '''
                testWeights:
                    "Test 1": 100
                    "Test 2": 100
            '''


App\Model\Entity\Exercise:
    demoExercise:
        __construct:
            create:
                - @demoAdmin
                - @demoGroup
                - @weightedTwoTests
        difficulty: "moderate"
        addLocalizedText:
            - @demoExerciseText
        runtimeEnvironments:
            - @CRuntime
            - @JavaRuntime
        hardwareGroups:
            - @demoHWGroup
            - @demoHWGroup2
        addExerciseLimits:
            - @demoExerciseLimits
        exerciseEnvironmentConfigs:
            - @demoEnvironmentConfig
            - @demoJavaEnvironmentConfig
        exerciseTests:
            - @demoExerciseTest1
            - @demoExerciseTest2
        exerciseConfig: @demoExerciseConfig
        isLocked: false
        isPublic: true

    "sampleExercise_{alfa, beta, alfred, aloha}":
        __construct:
            create:
