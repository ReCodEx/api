App\Model\Entity\LocalizedExercise:
    demoExerciseText:
        __construct:
            - en
            - "Convex hull"
            - ""
            - ""
    "sampleExerciseText":
        __construct:
            - en
            - An exercise
            - ""
            - ""
    privateAdminExerciseText:
        __construct:
            - en
            - "Another exercise"
            - ""
            - ""
    publicSupervisor2ExerciseText:
        __construct:
            - en
            - "group supervisor alalal"
            - ""
            - ""

App\Model\Entity\ExerciseScoreConfig:
    weightedTwoTests:
        __construct:
            - 'weighted'
            - testWeights:
                "Test 1": 100
                "Test 2": 100


App\Model\Entity\Exercise:
    demoExercise:
        __construct:
            create:
                - @demoAdmin
                - @demoGroup
                - @weightedTwoTests
        difficulty: "moderate"
        addLocalizedText: @demoExerciseText
        runtimeEnvironments:
            - @CRuntime
            - @JavaRuntime
        hardwareGroups:
            - @demoHWGroup
            - @demoHWGroup2
        addExerciseLimits: @demoExerciseLimits
        exerciseEnvironmentConfigs:
            - @demoEnvironmentConfig
            - @demoJavaEnvironmentConfig
        exerciseTests:
            - @demoExerciseTest1
            - @demoExerciseTest2
        exerciseConfig: @demoExerciseConfig
        isPublic: true
        __calls:
            - setLocked: [ false ]

    archivedExercise:
        __construct:
            create:
                - @demoGroupSupervisor
                - @demoGroup
                - @weightedTwoTests
        difficulty: "moderate"
        addLocalizedText: @demoExerciseText
        runtimeEnvironments:
            - @JavaRuntime
        hardwareGroups:
            - @demoHWGroup
        addExerciseLimits: @demoExerciseLimits
        exerciseEnvironmentConfigs:
            - @demoJavaEnvironmentConfig
        exerciseTests:
            - @demoExerciseTest1
            - @demoExerciseTest2
        exerciseConfig: @demoExerciseConfig
        isPublic: true
        __calls:
            - setLocked: [ false ]
            - setArchivedAt: [ "<dateTimeBetween('-10 months', '-1 months')>" ]

    "sampleExercise_{alfa, beta, alfred, aloha}":
        __construct:
            create:
                - @demoAdmin
                - @demoGroup
                - @weightedTwoTests
        difficulty: "easy"
        addLocalizedText: @sampleExerciseText
        runtimeEnvironments:
            - @CRuntime
            - @JavaRuntime
        hardwareGroups:
            - @demoHWGroup
            - @demoHWGroup2
        addExerciseLimits: @demoExerciseLimits
        exerciseEnvironmentConfigs:
            - @demoEnvironmentConfig
            - @demoJavaEnvironmentConfig
        exerciseTests:
            - @demoExerciseTest1
            - @demoExerciseTest2
        exerciseConfig: @demoExerciseConfig
        isPublic: true
        __calls:
            - setLocked: [ false ]

    privateAdminExercise:
        __construct:
            create:
                - @demoAdmin
                - @demoChildGroup
                - @weightedTwoTests
        difficulty: "moderate"
        addLocalizedText: @privateAdminExerciseText
        runtimeEnvironments:
            - @CRuntime
            - @JavaRuntime
            - @MonoRuntime
        hardwareGroups:
            - @demoHWGroup
            - @demoHWGroup2
        addExerciseLimits: @demoExerciseLimits
        exerciseEnvironmentConfigs:
            - @demoEnvironmentConfig
            - @demoJavaEnvironmentConfig
            - @demoMonoEnvironmentConfig
        exerciseTests:
            - @demoExerciseTest1
            - @demoExerciseTest2
        exerciseConfig: @demoExerciseConfig
        isPublic: false
        __calls:
            - setLocked: [ false ]

    publicSupervisor2Exercise:
        __construct:
            create:
                - @demoGroupSupervisor2
                - @demoGroup
                - @weightedTwoTests
        difficulty: "moderate"
        addLocalizedText: @publicSupervisor2ExerciseText
        runtimeEnvironments:
            - @CRuntime
            - @JavaRuntime
        hardwareGroups:
            - @demoHWGroup
            - @demoHWGroup2
        addExerciseLimits: @demoExerciseLimits
        exerciseEnvironmentConfigs:
            - @demoEnvironmentConfig
            - @demoJavaEnvironmentConfig
        exerciseTests:
            - @demoExerciseTest1
            - @demoExerciseTest2
        exerciseConfig: @demoExerciseConfig
        isPublic: true
        __calls:
            - setLocked: [ false ]

App\Model\Entity\ReferenceExerciseSolution:
    demoReferenceExerciseSolution:
        __construct:
            - @demoExercise
            - @demoGroupSupervisor
            - Brute force solution
            - @CRuntime
        visibility: 1
    privateReferenceExerciseSolution:
        __construct:
            - @demoExercise
            - @anotherSupervisor
            - Private solution of another supervisor
            - @CRuntime
        visibility: 0

App\Model\Entity\SolutionEvaluation:
    "demoReferenceExerciseSolutionEvaluationEvaluation{1..2}":
        __construct: false
        evaluatedAt: "<dateTime()>"
        initFailed: false
        score: 0.95
        scoreConfig: @weightedTwoTests
        points: 10
        initiationOutputs: ""

App\Model\Entity\ReferenceSolutionSubmission:
    "demoReferenceExerciseSolutionEvaluation{1..2}":
        __construct:
            - @demoReferenceExerciseSolution
            - @demoHWGroup
            - @demoAdmin
        evaluation: "@demoReferenceExerciseSolutionEvaluationEvaluation<current()>"

App\Model\Entity\AttachmentFile:
    demoExerciseAttachmentFile:
        __construct:
            - Our additional file
            - "<dateTimeBetween('-3 years', '-1 years')>"
            - 42
            - @demoAdmin
            - @demoExercise

